{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Jest: Unit Test File",
			"program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
			"args": [
				"--config",
				"test/unit/jest.config.js",
				"--runInBand",
				"/${fileBasename}"
			],
			"runtimeArgs": [
				"--nolazy"
			],
			"console": "internalConsole",
			"env": {
				"NODE_ENV": "test"
			},
			"cwd": "${workspaceRoot}",
			"sourceMaps": true,
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Jest: Unit Test All",
			"program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
			"args": [
				"--config",
				"test/unit/jest.config.js",
				"--runInBand"
			],
			"runtimeArgs": [
				"--nolazy"
			],
			"console": "internalConsole",
			"env": {
				"NODE_ENV": "test"
			},
			"sourceMaps": true,
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Jest: End-to-End Dev Test File",
			"program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
			"args": [
				"--config",
				"test/end-to-end/dev/jest.config.js",
				"--runInBand",
				"${fileBasename}"
			],
			"runtimeArgs": [
				"--nolazy"
			],
			"console": "internalConsole",
			"env": {
				"NODE_ENV": "test"
			},
			"cwd": "${workspaceRoot}",
			"sourceMaps": true,
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Jest: End-to-End Dev Test All",
			"program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
			"args": [
				"--config",
				"test/end-to-end/dev/jest.config.js",
				"--runInBand"
			],
			"runtimeArgs": [
				"--nolazy"
			],
			"console": "internalConsole",
			"env": {
				"NODE_ENV": "test"
			},
			"sourceMaps": true,
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Jest: End-to-End SSR Test File",
			"program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
			"args": [
				"--config",
				"test/end-to-end/ssr/jest.config.js",
				"--runInBand",
				"${fileBasename}"
			],
			"runtimeArgs": [
				"--nolazy"
			],
			"console": "internalConsole",
			"env": {
				"NODE_ENV": "test"
			},
			"cwd": "${workspaceRoot}",
			"sourceMaps": true,
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Jest: End-to-End SSR Test All",
			"program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
			"args": [
				"--config",
				"test/end-to-end/ssr/jest.config.js",
				"--runInBand"
			],
			"runtimeArgs": [
				"--nolazy"
			],
			"console": "internalConsole",
			"env": {
				"NODE_ENV": "test"
			},
			"sourceMaps": true,
		},
		{
			"name": "Attach to Chrome",
			"type": "chrome",
			"request": "attach",
			"port": 9222,
			"url": "http://localhost:3000",
			"webRoot": "${workspaceRoot}"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Debug current JS file in Node",
			"program": "${file}",
		}
	]
}